Public Class historial
    Private _idhistorial As Integer
    Private _estado As Integer
    Private _horasactivo As Double
    Private _fecharevision As DateTime
    Private _fechacaida As DateTime
    Private _causal As String
    Private _idservidor As Integer

    Public Property idhistorial() As Integer
        Get
            Return _idhistorial
        End Get
        Set(ByVal value As Integer)
            _idhistorial = value
        End Set
    End Property

    Public Property estado() As Integer
        Get
            Return _estado
        End Get
        Set(value As Integer)
            _estado = value
        End Set
    End Property
    Public Property horasactivo() As Double
        Get
            Return _horasactivo
        End Get
        Set(value As Double)
            _horasactivo = value
        End Set
    End Property

    Public Property fecharevision() As DateTime
        Get
            Return _fecharevision
        End Get
        Set(value As DateTime)
            _fecharevision = value
        End Set
    End Property
    Public Property fechacaida() As DateTime
        Get
            Return _fechacaida
        End Get
        Set(value As DateTime)
            _fechacaida = value
        End Set
    End Property
    Public Property causal() As String
        Get
            Return _causal
        End Get
        Set(ByVal value As String)
            _causal = value
        End Set
    End Property

    Public Property idservidor() As Integer
        Get
            Return _idservidor
        End Get
        Set(ByVal value As Integer)
            _idservidor = value
        End Set
    End Property
End Class
